{
    "namespace": "asa_start_anims",

    //Bottom Panel
    "asa_bottom_panel_entrance_1": {
        "anim_type": "offset",
        "from": [ 0, 150 ],
        "to": [ 0, 150 ],
        "duration": 0.7,
        "play_event": "screen.entrance_pop",
        "next": "@asa_start_anims.asa_bottom_panel_entrance"
    },

    "asa_bottom_panel_entrance_2": {
        "anim_type": "offset",
        "from": [ 0, 150 ],
        "to": [ 0, 150 ],
        "duration": 0.7,
        "play_event": "screen.entrance_push",
        "next": "@asa_start_anims.asa_bottom_panel_entrance"
    },

    "asa_bottom_panel_entrance": {
        "anim_type": "offset",
        "easing": "out_circ",
        "from": [ 0, 150 ],
        "to": [ 0, -2 ],
        "duration": 1.2
    },

    "asa_bottom_panel_exit_1": {
        "anim_type": "offset",
        "easing": "in_sine",
        "from": [ 0, -2 ],
        "to": [ 0, 150 ],
        "duration": 0.6,
        "play_event": "screen.exit_push"
    },

    "asa_bottom_panel_exit_2": {
        "anim_type": "offset",
        "easing": "in_sine",
        "from": [ 0, -2 ],
        "to": [ 0, 150 ],
        "duration": 0.6,
        "play_event": "screen.exit_pop"
    },

    //Start Gamertag Anims
    "asa_start_gamertag_entrance_1": {
        "anim_type": "offset",
        "duration": 1.2,
        "easing": "out_back",
        "from": [ -75, 2 ],
        "to": [ 2, 2 ],
        "play_event": "screen.entrance_pop"
    },

    "asa_start_gamertag_entrance_2": {
        "anim_type": "offset",
        "duration": 1.2,
        "easing": "out_back",
        "from": [ -75, 2 ],
        "to": [ 2, 2 ],
        "play_event": "screen.entrance_push"
    },

    "asa_start_gamertag_exit_1": {
        "anim_type": "offset",
        "duration": 0.7,
        "easing": "in_sine",
        "from": [ 2, 2 ],
        "to": [ -32, 2 ],
        "play_event": "screen.exit_pop"
    },

    "asa_start_gamertag_exit_2": {
        "anim_type": "offset",
        "duration": 0.7,
        "easing": "in_sine",
        "from": [ 2, 2 ],
        "to": [ -32, 2 ],
        "play_event": "screen.exit_push"
    },

    //Start BG Screen
    "asa_start_bg_1": {
        "anim_type": "wait",
        "duration": 5,
        "next": "@asa_start_anims.asa_start_bg_1_1"
    },

    "asa_start_bg_1_1": {
        "anim_type": "offset",
        "duration": 2.5,
        "from": [ 0, 0 ],
        "to": [ "5%", 0 ],
        "easing": "in_sine",
        "next": "@asa_start_anims.asa_start_bg_2"
    },

    "asa_start_bg_2": {
        "anim_type": "offset",
        "duration": 2.5,
        "from": [ "5%", 0 ],
        "to": [ "10%", 0 ],
        "easing": "out_sine",
        "next": "@asa_start_anims.asa_bg_wait"
    },

    "asa_bg_wait": {
        "anim_type": "wait",
        "duration": 1,
        "next": "@asa_start_anims.asa_start_bg_3"
    },

    "asa_start_bg_3": {
        "anim_type": "offset",
        "duration": 5,
        "from": [ "10%", 0 ],
        "to": [ "0%", 0 ],
        "easing": "in_sine",
        "next": "@asa_start_anims.asa_start_bg_4"
    },

    "asa_start_bg_4": {
        "anim_type": "offset",
        "duration": 5,
        "from": [ "0%", 0 ],
        "to": [ "-10%", 0 ],
        "easing": "out_sine",
        "next": "@asa_start_anims.asa_bg_wait_2"
    },

    "asa_bg_wait_2": {
        "anim_type": "wait",
        "duration": 1,
        "next": "@asa_start_anims.asa_start_bg_5"
    },

    "asa_start_bg_5": {
        "anim_type": "offset",
        "duration": 5,
        "from": [ "-10%", 0 ],
        "to": [ "0%", 0 ],
        "easing": "in_sine",
        "next": "@asa_start_anims.asa_start_bg_6"
    },

    "asa_start_bg_6": {
        "anim_type": "offset",
        "duration": 5,
        "from": [ "0%", 0 ],
        "to": [ "10%", 0 ],
        "easing": "out_sine",
        "next": "@asa_start_anims.asa_bg_wait"
    },

    //Copyright
    "asa_better_copyright_entrance_wait": {
        "anim_type": "offset",
        "easing": "out_sine",
        "duration": "$asa_duration",
        "from": "$asa_from",
        "to": "$asa_from",
        "play_event": "screen.entrance_pop",
        "next": "@asa_start_anims.asa_better_copyright_entrance"
    },

    "asa_better_copyright_entrance_wait_2": {
        "anim_type": "offset",
        "easing": "out_sine",
        "duration": "$asa_duration",
        "from": "$asa_from",
        "to": "$asa_from",
        "play_event": "screen.entrance_push",
        "next": "@asa_start_anims.asa_better_copyright_entrance_2"
    },

    "asa_better_copyright_entrance": {
        "anim_type": "offset",
        "easing": "out_back",
        "duration": 1.2,
        "from": "$asa_from",
        "to": "$asa_to",
        "end_event": "screen.entrance_end"
    },

    "asa_better_copyright_entrance_2": {
        "anim_type": "offset",
        "easing": "out_sine",
        "duration": 0.8,
        "from": "$asa_from",
        "to": "$asa_to",
        "end_event": "screen.entrance_end"
    },

    "asa_better_copyright_exit": {
        "anim_type": "offset",
        "easing": "linear",
        "duration": 0.6,
        "from":  "$asa_to",
        "to": "$asa_from",
        "play_event": "screen.exit_pop",
        "end_event": "screen.exit_end"
    },

    "asa_better_copyright_exit_2": {
        "anim_type": "offset",
        "easing": "linear",
        "duration": 0.6,
        "from":  "$asa_to",
        "to": "$asa_from",
        "play_event": "screen.exit_push",
        "end_event": "screen.exit_end"
    },

    //Mojang Copyright
    "asa_mojang_copyright_entrance_wait": {
        "anim_type": "offset",
        "easing": "out_sine",
        "duration": "$asa_duration",
        "from": [ -200, -1 ],
        "to": [ -200, -1 ],
        "play_event": "screen.entrance_pop",
        "next": "@asa_start_anims.asa_mojang_copyright_entrance"
    },

    "asa_mojang_copyright_entrance_wait_2": {
        "anim_type": "offset",
        "easing": "out_sine",
        "duration": "$asa_duration",
        "from": [ -200, -1 ],
        "to": [ -200, -1 ],
        "play_event": "screen.entrance_push",
        "next": "@asa_start_anims.asa_mojang_copyright_entrance_2"
    },

    "asa_mojang_copyright_entrance": {
        "anim_type": "offset",
        "easing": "out_back",
        "duration": 1.2,
        "from": [ -200, -1 ],
        "to": [ 3, -1 ],
        "end_event": "screen.entrance_end"
    },

    "asa_mojang_copyright_entrance_2": {
        "anim_type": "offset",
        "easing": "out_back",
        "duration": 1.2,
        "from": [ -200, -1 ],
        "to": [ 3, -1 ],
        "end_event": "screen.entrance_end"
    },

    "asa_mojang_copyright_exit": {
        "anim_type": "offset",
        "easing": "linear",
        "duration": 0.6,
        "from":  [ 3, -1 ],
        "to": [ -200, -1 ],
        "play_event": "screen.exit_pop",
        "end_event": "screen.exit_end"
    },

    "asa_mojang_copyright_exit_2": {
        "anim_type": "offset",
        "easing": "linear",
        "duration": 0.6,
        "from":  [ 3, -1 ],
        "to": [ -200, -1 ],
        "play_event": "screen.exit_push",
        "end_event": "screen.exit_end"
    },

    //Play Panel
    "asa_play_panel_copyright_entrance": {
        "anim_type": "offset",
        "easing": "out_circ",
        "duration": 1.4,
        "from": [ 0, "100%" ],
        "to": [ 0, 15 ],
        "play_event": "screen.entrance_pop",
        "end_event": "screen.entrance_end"
    },

    "asa_play_panel_copyright_entrance_2": {
        "anim_type": "offset",
        "easing": "out_circ",
        "duration": 1.4,
        "from": [ 0, "100%" ],
        "to": [ 0, 15 ],
        "play_event": "screen.entrance_push",
        "end_event": "screen.entrance_end"
    },

    "asa_play_panel_copyright_exit": {
        "anim_type": "offset",
        "easing": "linear",
        "duration": 0.6,
        "from":  [ 0, 15 ],
        "to": [ 0, "100%" ],
        "play_event": "screen.exit_pop",
        "end_event": "screen.exit_end"
    },

    "asa_play_panel_copyright_exit_2": {
        "anim_type": "offset",
        "easing": "linear",
        "duration": 0.6,
        "from":  [ 0, 15 ],
        "to": [ 0, "100%" ],
        "play_event": "screen.exit_push",
        "end_event": "screen.exit_end"
    },

    //Game Title
    "asa_title_panel_copyright_entrance": {
        "anim_type": "offset",
        "easing": "out_cubic",
        "duration": 0.5,
        "from": [ 0, "-100%" ],
        "to": [ 0, -50 ],
        "next": "@asa_start_anims.asa_title_panel_copyright_entrance_no_2",
        "play_event": "screen.entrance_pop"
    },

    "asa_title_panel_copyright_entrance_no_2": {
        "anim_type": "offset",
        "easing": "in_back",
        "duration": 0.7,
        "from": [ 0, -50 ],
        "to": [ 0, -5 ],
        "next": "@asa_start_anims.asa_title_panel_copyright_entrance_no_3"
    },

    "asa_title_panel_copyright_entrance_no_3": {
        "anim_type": "offset",
        "easing": "out_back",
        "duration": 0.7,
        "from": [ 0, -5 ],
        "to": [ 0, -12 ],
        "end_event": "screen.entrance_end"
    },

    "asa_title_panel_copyright_entrance_2": {
        "anim_type": "offset",
        "easing": "out_cubic",
        "duration": 0.5,
        "from": [ 0, "-100%" ],
        "to": [ 0, -50 ],
        "next": "@asa_start_anims.asa_title_panel_copyright_entrance_no_2",
        "play_event": "screen.entrance_push"
    },

    "asa_title_panel_copyright_exit": {
        "anim_type": "offset",
        "easing": "linear",
        "duration": 0.6,
        "from":  [ 0, -12 ],
        "to": [ 0, "-100%" ],
        "play_event": "screen.exit_pop",
        "end_event": "screen.exit_end"
    },

    "asa_title_panel_copyright_exit_2": {
        "anim_type": "offset",
        "easing": "linear",
        "duration": 0.6,
        "from":  [ 0, -12 ],
        "to": [ 0, "-100%" ],
        "play_event": "screen.exit_push",
        "end_event": "screen.exit_end"
    }
}