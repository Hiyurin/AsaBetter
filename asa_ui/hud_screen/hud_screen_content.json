    //Asa Exp Percent
    "asa_exp_bindings_percent": {
        "type": "label",
        "color": [ 0.5, 1, 0 ],
        "shadow": true,
        "localize": false,
        "bindings": [
            {
                "binding_name": "#exp_progress",
                "binding_type": "global"
            },
            {
                "binding_type": "view",
                "source_property_name": "(((#exp_progress > $min_exp_bar) and (#exp_progress < $max_exp_bar)) or (#exp_progress = $max_exp_bar))",
                "target_property_name": "#visible"
            }
        ]
    },

    "asa_expbar_percent_stack": {
        "type": "stack_panel",
        "size": [ "100%c", "100%cm" ],
        "anchor_from": "left_middle",
        "anchor_to": "left_middle",
        "offset": [ "-100%x - 7px", -1 ],
        "orientation": "horizontal",
        "controls": [
            {
                "@asa_exp_progress": {}
            },
            {
                "@asa_exp_bindings_percent": {
                    "type": "label",
                    "text": "%",
                    "$min_exp_bar": 0,
                    "$max_exp_bar": 1
                }
            }
        ]
    },

