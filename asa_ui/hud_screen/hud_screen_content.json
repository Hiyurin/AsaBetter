{
    "namespace": "asa_hud_content",

    //Hotbar
    "asa_hotbar_panel": {
        "type": "panel",
        "size": [ 180, 22 ],
        "controls": [
            {
                "connected@asa_connected_hotbar": {}
            },
            {
                "default@hud.hotbar_grid": {}
            },
            {
                "rgb_default@asa_rgb_default_hotbar_stack": {}
            }
        ]
    },

    "asa_connected_hotbar": {
        "type": "image",
        "size": [ 180, 22 ],
        "texture": "asa_textures/ui/asa_connected_hotbar",
        "alpha": 0.7,
        "anchor_from": "bottom_middle",
        "anchor_to": "bottom_middle",
        "bindings": [
            {
                "binding_type": "view",
                "source_control_name": "asa_connected_hotbar",
                "source_property_name": "#toggle_state",
                "target_property_name": "#visible"
            }
        ]
    },

    "asa_rgb_default_hotbar_stack": {
        "type": "stack_panel",
        "size": [ 180, 22 ],
        "anchor_from": "bottom_middle",
        "anchor_to": "bottom_middle",
        "orientation": "horizontal",
        "$custom_anim_duration": 0.4,
        "bindings": [
            {
                "binding_type": "view",
                "source_control_name": "asa_rgb_default_hotbar",
                "source_property_name": "#toggle_state",
                "target_property_name": "#visible"
            }
        ],
        "controls": [
            {
                "1@asa_rgb_hotbar_image": {
                    "$asa_hotbar_rgb_color": "@asa_extra_animation.rgb_3"
                }
            },
            {
                "2@asa_rgb_hotbar_image": {
                    "$asa_hotbar_rgb_color": "@asa_extra_animation.rgb_2"
                }
            },
            {
                "3@asa_rgb_hotbar_image": {
                    "$asa_hotbar_rgb_color": "@asa_extra_animation.rgb_1"
                }
            },
            {
                "4@asa_rgb_hotbar_image": {
                    "$asa_hotbar_rgb_color": "@asa_extra_animation.rgb_6"
                }
            },
            {
                "5@asa_rgb_hotbar_image": {
                    "$asa_hotbar_rgb_color": "@asa_extra_animation.rgb_5"
                }
            },
            {
                "6@asa_rgb_hotbar_image": {
                    "$asa_hotbar_rgb_color": "@asa_extra_animation.rgb_4"
                }
            },
            {
                "7@asa_rgb_hotbar_image": {
                    "$asa_hotbar_rgb_color": "@asa_extra_animation.rgb_3"
                }
            },
            {
                "8@asa_rgb_hotbar_image": {
                    "$asa_hotbar_rgb_color": "@asa_extra_animation.rgb_2"
                }
            },
            {
                "9@asa_rgb_hotbar_image": {
                    "$asa_hotbar_rgb_color": "@asa_extra_animation.rgb_1"
                }
            }
        ]
    },

    "asa_rgb_hotbar_image": {
        "type": "panel",
        "size": [ 20, 22 ],
        "controls": [
            {
                "image": {
                    "type": "image",
                    "size": [ 21, 23 ],
                    "color": "$asa_hotbar_rgb_color",
                    "texture": "asa_textures/ui/asa_default_hotbar"
                }
            }
        ]
    },

    //Shortcut Button
    "asa_shortcut_button_stack": {
        "type": "stack_panel",
        "size": [ "100%c", "100%cm" ],
        "orientation": "horizontal",
        "anchor_from": "bottom_right",
        "anchor_to": "bottom_right",
        "bindings": [
            {
                "binding_name": "#inventory_touch_button",
                "binding_name_override": "#visible",
                "binding_type": "global"
            }
        ],
        "controls": [
            {
                "@asa_start_button_template.asa_start_button_template": {
                    "size": [ 24, 24 ],
                    "$pressed_button_name": "button.hide_paperdoll_hud",
                    "$button_text": "F8",
                    "bindings": [
                        {
                            "binding_type": "view",
                            "source_control_name": "asa_shortcut_button",
                            "source_property_name": "#toggle_state",
                            "target_property_name": "#visible"
                        }
                    ]
                }
            },
            {
                "empty_panel": {
                    "type": "panel",
                    "size": [ 1, 1 ],
                    "bindings": [
                        {
                            "binding_type": "view",
                            "source_control_name": "asa_shortcut_button",
                            "source_property_name": "#toggle_state",
                            "target_property_name": "#visible"
                        }
                    ]
                }
            },
            {
                "@asa_start_button_template.asa_start_button_template": {
                    "size": [ 24, 24 ],
                    "$pressed_button_name": "button.hide_gui_all",
                    "$button_text": "F1",
                    "bindings": [
                        {
                            "binding_type": "view",
                            "source_control_name": "asa_shortcut_button",
                            "source_property_name": "#toggle_state",
                            "target_property_name": "#visible"
                        }
                    ]
                }
            }
        ]
    },

    //Bossbar Percent
    "asa_bossbar_percent_bindings": {
        "$asa_bossbar_bindings": "#progress_percentage",
        "type": "label",
        "localize": false,
        "layer": 10,
        "shadow": true,
        "color": "$d_color_format",
        "bindings": [
            {
                "binding_name": "$asa_bossbar_bindings",
                "binding_type": "collection",
                "binding_collection_name": "boss_bars"
            },
            {
                "binding_type": "view",
                "source_property_name": "((($asa_bossbar_bindings > $health_min) and ($asa_bossbar_bindings < $health_max)) or ($asa_bossbar_bindings = $health_min))",
                "target_property_name": "#visible"
            }
        ]
    },

    "asa_bossbar_percent": {
        "type": "panel",
        "size": [ "100%cm", "100%cm" ],
        "anchor_from": "top_middle",
        "anchor_to": "top_middle",
        "offset": [ 113, 3 ],
        "bindings": [
            {
                "binding_type": "view",
                "source_control_name": "asa_health_bar_percent",
                "source_property_name": "#toggle_state",
                "target_property_name": "#visible"
            }
        ],
        "controls": [
            {
                "@asa_bossbar_background": {}
            }
        ]
    },

    "asa_bossbar_background": {
        "type": "image",
        "size": [ "100%cm + 4px", "100%cm + 4px" ],
        "texture": "asa_textures/ui/asa_background_white",
        "color": [ 0, 0, 0 ],
        "alpha": 0.7,
        "bindings": [
            {
                "binding_name": "#bar_visible",
                "binding_type": "collection",
                "binding_collection_name": "boss_bars",
                "binding_name_override": "#visible"
            }
        ],
        "controls": [
            {
                "@asa_bossbar_percent_panel": {}
            }
        ]
    },

