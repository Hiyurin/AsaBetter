{
    "namespace": "asa_progress_screen_panel",
    //Loading World Percent
    "percentage_bindings": {
        "size": [
            "100%",
            "100%"
        ],
        "$total": "#loading_bar_total_amount",
        "$current": "#loading_bar_current_amount",
        "bindings": [
            {
                "binding_name": "$total"
            },
            {
                "binding_name": "$current"
            },
            {
                "binding_type": "view",
                "source_property_name": "((($current / $total) > $min) and (($current / $total) < $max))",
                "target_property_name": "#visible"
            }
        ]
    },

    "percen_label@percentage_bindings": {
        "anchor_from": "top_left",
        "anchor_to": "top_left",
        "type": "stack_panel",
        "layer": 100,
        "orientation": "horizontal",
        "size": [
            "100%c",
            12
        ],
        "$number_text|default": "",
        "controls": [
            {
                "number": {
                    "type": "label",
                    "text": "$number_text",
                    "shadow": true,
                    "font_type": "smooth",
                    "font_scale_factor": 1.2,
                    "layer": 100,
                    "localize": false,
                    "size": [
                        "default",
                        12
                    ]
                }
            },
            {
                "percentage": {
                    "type": "label",
                    "text": "%",
                    "shadow": true,
                    "font_type": "smooth",
                    "font_scale_factor": 1.2,
                    "layer": 100,
                    "localize": false,
                    "size": [
                        "default",
                        12
                    ]
                }
            }
        ]
    },

