    },

    "world_saving_progress_screen": {
        "$screen_content": "progress.progress_screen_content",
        "$screen_bg_content": "asa_progress_screen_panel.background_with_panel",
        "$background_image": "asa_textures/asa_background/loading_bg/loading_bg",
